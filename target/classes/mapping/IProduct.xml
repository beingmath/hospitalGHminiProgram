<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.mapper.IProduct">
     <select id="selectProducts" resultType="com.example.demo.entity.Product" parameterType="Integer">
        select * from product where parentid=#{parentid}
    </select>

</mapper>
